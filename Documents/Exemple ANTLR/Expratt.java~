
class Expratt {

	 public Code code;
	 public Type type;
	 public Tokatt place;
	 public TableSymboles tab;

	 public Expratt(Type ty, Code cod, Tokatt pl){
		  code = cod;
		  type = ty;
		  place = pl;
	 }

	 public Expratt(Type ty, Code cod, Tokatt pl, TableSymboles tab){
		  code = cod;
		  type = ty;
		  place = pl;
		  tab = tab;
	 }

}
