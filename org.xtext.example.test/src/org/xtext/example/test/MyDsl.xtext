grammar org.xtext.example.test.MyDsl with org.eclipse.xtext.common.Terminals

generate myDsl "http://www.xtext.org/example/test/MyDsl"

Model:
	functions+=Function*;
	
Function:
	'function' name=ID ':'
	reads += Read
	'%'
	commands+=Command*
	'%'
	writes+=Write
		;

Read :
	'read' name+=ID (',' name+=ID)*
;
Write :
	'write' name+=ID (',' name+=ID)*
;
Command:
	Nop | Affectation 
;
Nop:
	name='nop'
;
Affectation:
	(name=ID ':=' valeur=INT) 
;